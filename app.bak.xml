<?xml version="1.0" encoding="UTF-8"?>
<app xmlns="http://jctrl.org/schema/urimap" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<default dataType="xml" viewType="html" entry="home" lang="cn" />

	<language>
		<group name="cn" path="language/cn/" >
			<file>default.xml</file>
		</group>
		<group name="en" path="language/en/" >
			<file>default.xml</file>
		</group>
	</language>

	<map>
		<entry key="{x}" >
		</entry>
	</map>
	
	<views>
		<group type="route" path="views/">
			<view name="home">
				<script>home.js</script>
			</view>
			<view name="index">
				<script>index.js</script>
			</view>
		</group>
	</views>
	
</app>